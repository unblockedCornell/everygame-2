{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": [
    "google"
  ],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "ignorePatterns": [
    "dist/**/*.js",
    "src/music.js",
    "src/lib/shaders.js",
    "src/zzfx.js",
    "src/zzfxm.js",
    "zzfxm/*"
  ],
  "rules": {
    "max-len": [
      "error",
      {
        "code": 160
      }
    ],
    "no-unused-vars": [
      "off"
    ]
  }
}